<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="350dp"
                android:height="350dp" />
            <solid android:color="#ffd9d9" />
        </shape>
    </item>
    <item android:left="20dp"
        android:top="20dp"
        android:right="20dp"
        android:bottom="20dp">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="#fdb6b6" />
        </shape>
    </item>
    <item
        android:left="40dp"
        android:top="40dp"
        android:right="40dp"
        android:bottom="40dp">
        <shape
            android:shape="oval"
            android:thickness="25dp"
            android:useLevel="false">
            <solid android:color="#f4a0a0" />
        </shape>
    </item>
</layer-list>